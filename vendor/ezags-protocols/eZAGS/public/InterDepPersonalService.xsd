	<!--Используемые типы данных-->
	<!-- Фильтры-->
	<xs:complexType name="BirthFilterRequest">
		<xs:annotation>
			<xs:documentation>Фильтр для поиска актовой записи о рождении</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="1">
			<xs:element ref="smev:Message"/>
			<xs:element name="MessageData">
				<xs:annotation>
					<xs:documentation>Служебный блок-обертка данных сообщения в СМЭВ</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="1">
						<xs:element name="AppData">
							<xs:annotation>
								<xs:documentation>Блок структурированных сведений</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence minOccurs="1">
									<xs:element ref="ns2:birthFilterRequestObj"/>
									<xs:element ref="ds:Signature" minOccurs="0"/>
								</xs:sequence>
								<xs:attribute name="id" type="xs:string" fixed="AppData"/>
							</xs:complexType>
						</xs:element>
						<xs:element ref="smev:AppDocument" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MarriageFilterRequest">
		<xs:annotation>
			<xs:documentation>Фильтр для поиска актовой записи о заключении брака</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="1">
			<xs:element ref="smev:Message"/>
			<xs:element name="MessageData">
				<xs:annotation>
					<xs:documentation>Служебный блок-обертка данных сообщения в СМЭВ</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="1">
						<xs:element name="AppData">
							<xs:annotation>
								<xs:documentation>Блок структурированных сведений</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence minOccurs="1">
									<xs:element ref="ns2:marriageFilterRequestObj"/>
									<xs:element ref="ds:Signature" minOccurs="0"/>
								</xs:sequence>
								<xs:attribute name="id" type="xs:string" fixed="AppData"/>
							</xs:complexType>
						</xs:element>
						<xs:element ref="smev:AppDocument" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DivorceFilterRequest">
		<xs:annotation>
			<xs:documentation>Фильтр для поиска актовой записи о расторжении брака</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="1">
